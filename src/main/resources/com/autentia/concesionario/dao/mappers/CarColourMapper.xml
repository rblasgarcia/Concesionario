<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.autentia.concesionario.dao.mappers.CarColourMapper">

	<select id="get" resultType="com.autentia.concesionario.model.CarColour">
		select * from car_colours where id = #{id}
	</select>

	<select id="getAllCarColourNames" resultType="string">
		select colour from car_colours
	</select>

</mapper>
